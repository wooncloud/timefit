### 4. 고객 OAuth 로그인 (Google 가정)
POST http://localhost:8080/api/customer/auth/oauth
Content-Type: application/json

{
  "provider": "GOOGLE",
  "accessToken": "ya29.test_token",
  "oauthId": "1234567890"
}

> {%
    client.test("고객 OAuth 로그인 성공", function() {
        client.assert(response.status === 200, "응답 코드가 200이어야 함");
        client.assert(response.headers.valueOf("x-client-token"), "x-client-token 헤더가 있어야 함");
    });
    client.global.set("customerToken", response.headers.valueOf("x-client-token"));
    console.log("고객 토큰:", client.global.get("customerToken"));
%}

