### 1. 헬스체크 - 서버 동작 확인
GET http://localhost:8080/api/health

### 2. 업체 회원가입
POST http://localhost:8080/api/business/auth/signup
Content-Type: application/json

{
  "email": "fresh@example.com",
  "password": "Password123!",
  "name": "신규사장",
  "phoneNumber": "010-1111-2222",
  "businessName": "신규카페",
  "businessType": "카페",
  "businessNumber": "111-22-33333",
  "address": "서울시 강남구 테헤란로 111",
  "contactPhone": "02-1111-2222",
  "description": "새로 오픈한 카페입니다"
}

> {%
    client.test("업체 회원가입 성공", function() {
        client.assert(response.status === 201, "응답 코드가 201이어야 함");
        client.assert(response.headers.valueOf("x-client-token"), "x-client-token 헤더가 있어야 함");
    });
    client.global.set("businessToken", response.headers.valueOf("x-client-token"));
    console.log("업체 토큰:", client.global.get("businessToken"));
%}

